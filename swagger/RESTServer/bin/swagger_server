#!/home/khickman/cloudmesh/swagger/RESTServer/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'swagger-server==1.0.0','console_scripts','swagger_server'
__requires__ = 'swagger-server==1.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('swagger-server==1.0.0', 'console_scripts', 'swagger_server')()
    )
